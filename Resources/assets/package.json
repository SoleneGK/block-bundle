{
  "name": "@umanit/block-bundle",
  "description": "Package for umanit/block-bundle",
  "license": "MIT",
  "version": "0.0.1",
  "symfony": {
    "controllers": {
      "blocks": {
        "main": "dist/blocks.js",
        "fetch": "lazy",
        "enabled": true
      },
      "item": {
        "main": "dist/item.js",
        "fetch": "lazy",
        "enabled": true
      },
      "sortable": {
        "main": "dist/sortable.js",
        "fetch": "lazy",
        "enabled": true
      }
    }
  },
  "scripts": {
    "build": "babel src -d dist"
  },
  "dependencies": {
    "sortablejs": "^1.10.2",
    "stimulus-use": "^0.50.0",
    "@hotwired/stimulus": "^3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-env": "^7.12.7",
    "core-js": "^3.0.0"
  },
  "peerDependencies": {
    "@hotwired/stimulus": "^3.0.0"
  },
  "keywords": ["symfony-ux"]
}
